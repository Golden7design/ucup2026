{
  "root": true,
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "no-undef": "off",
    "no-unused-vars": "off"
  },
  "ignorePatterns": ["node_modules/", "public/", "vendor/", "storage/"]
}